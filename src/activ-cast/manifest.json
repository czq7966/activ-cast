{
   "default_locale":"en",
   "name":"__MSG_manifest_name__",
   "author":"ND CZQ",
   "version":"1.8.1.0",
   "manifest_version":2,
   "minimum_chrome_version":"34",
   "description":"__MSG_manifest_description__",
   "short_name":"__MSG_manifest_short_name__",
   "homepage_url":"https://chrome.google.com/webstore/detail/prometheanÂ®-screen-share/kkepemkdiboiecpjlmdmighjkmfokbpm",
   "background":{
      "scripts":[
         "background/index.js"
      ],
      "persistent": true
   },
   "browser_action":{  
      "default_icon":"images/icons/128.png",
      "default_title":"__MSG_manifest_default_title__",
      "default_popup": "pages/dropdown/index.html"
   },
   "icons":{
      "16" : "images/icons/16.png",
      "32" : "images/icons/32.png",
      "48" : "images/icons/48.png",
      "64" : "images/icons/64.png",
      "128" : "images/icons/128.png"
   },
   "permissions":[
      "desktopCapture"
      ,"storage"
      ,"nativeMessaging"
      ,"idle"
      ,"contextMenus"
      ,"tabs"
      ,"<all_urls>"
      ,"activeTab"
      ,"tabCapture"
      ,"debugger"
   ],
   "options_ui": {
      "page": "pages/dropdown/options.html",
      "open_in_tab": true
   }
}